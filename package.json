    "jspdf": "^2.5.2",
    "moment": "^2.30.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.0",
    "proj4": "^2.17.0",
    "puppeteer": "^24.3.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
