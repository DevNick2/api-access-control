    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^10.4.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@turf/turf": "^7.2.0",
    "@types/jspdf": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/puppeteer": "^7.0.4",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.4",
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
